{
  "permissions": {
    "allow": [
      "Bash(poetry run python:*)",
      "Bash(poetry run pytest:*)",
      "Bash(dir:*)",
      "Bash(git log:*)",
      "Bash(git status:*)",
      "Bash(poetry show:*)"
    ],
    "deny": [
      "Bash(pip install:*)",
      "Bash(poetry add:*)",
      "Bash(poetry remove:*)",
      "Bash(git push:*)",
      "Bash(git pull:*)"
    ],
    "ask": [
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(rm:*"
    ]
  }
}
